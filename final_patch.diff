+u32 rtw_atoi(u8 *s)
+{
+	int num = 0, flag = 0;
+	u32 i;
+
+	for (i = 0; i <= strlen((char *)s); i++) {
+		if (s[i] >= '0' && s[i] <= '9') {
+			num = num * 10 + s[i] - '0';
+		} else if (s[0] == '-' && i == 0) {
+			flag = 1;
+		} else {
+			break;
+		}
+	}
+
+	if (flag == 1) {
+		num = num * -1;
+	}
+
+	return (num);
+}
