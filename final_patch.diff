-	/* Special cases: first chunk in the free list or memory completely full */
-	if (((uint8_t *)mem) < ((uint8_t *)h->FreeList) || !h->FreeList) {
-		/* Insert memory block before the current free list head */
-		prev = (PsramMemChunk *)mem;
