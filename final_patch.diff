-
-unsigned char *skb_put(struct sk_buff *skb, unsigned int len)
-{
-	unsigned char *tmp = skb->tail;
-	skb->tail += len;
-	skb->len += len;
-	if (skb->tail > skb->end) {
-		ASSERT(0);
-	}
-
-	return tmp;
-}
