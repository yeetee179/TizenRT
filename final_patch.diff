-	WIFI_MONITOR_TIMER_END(wlan_send_time, total_len);
-
-exit:
-	save_and_cli();
-	rltk_wlan_tx_dec(idx);
-	restore_flags();
-	return ret;
-#else
-	(void)idx;
-	(void)sg_list;
-	(void)sg_len;
-	(void)total_len;
-#endif
-	return -1;
-}
