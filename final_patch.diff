-	/* To avoid gcc warnings */
-	(void) pkt;
-
-	u8 *buf_star = NULL;
-	union recv_frame *precv_frame = NULL;
-
-	precv_frame = rxmem_to_recvframe((unsigned char *)buf_star);
-
-	return precv_frame;
