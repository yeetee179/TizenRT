-void Psram_reserved_heap_freemem(void *mem, int size)
-{
-	PsramMemChunk *prev;
-	struct Psram_Heap *h = &g_Psram_heap;
-	_irqL 	irqL;
-
-	rtw_enter_critical(&Psram_heap_lock, &irqL);
